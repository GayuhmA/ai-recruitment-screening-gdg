{
  "name": "ai-recruitment-monorepo",
  "version": "0.0.1",
  "description": "AI Recruitment Platform - Monorepo",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev:web": "npm --workspace @ai-recruitment/web run dev",
    "dev:api": "npm --workspace @ai-recruitment/api run dev:api",
    "dev:worker": "npm --workspace @ai-recruitment/api run dev:worker",
    "dev": "npm-run-all --parallel dev:api dev:worker dev:web",
    "build:web": "npm --workspace @ai-recruitment/web run build",
    "build:api": "npm --workspace @ai-recruitment/api run build",
    "prisma:migrate": "npm --workspace @ai-recruitment/api run prisma:migrate",
    "prisma:studio": "npm --workspace @ai-recruitment/api run prisma:studio"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "next": "^16.1.6"
  }
}
